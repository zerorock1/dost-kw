  \n         \n         \n                <agile>\n                  \n                      <img v-for=\"(foto, index) in fotosurl\" :key=\"index\" :src=\"foto.url\" class=\"slide\" style=\"max-width:100px;\" />\n                 \n                </agile>\n\n\n\n            </div>\n          </div>\n\n          <Formficha />\n\n          <div class=\"col-md-8\">\n            <div class=\"textos\">\n              <p class=\"descrip\">\n                {{ viviendaseleccionada.nbconservacion }}\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\n\nimport Formficha from './Formficha.vue'\nimport MoneyFormat from 'vue-money-format'\nimport { VueAgile } from 'vue-agile'\n\n\nexport default {\n    \n    name: 'Ficha',\n      data() {\n        return {\n            fotosurl: [],\n        };\n  },\n    computed: {\n        viviendaseleccionada() \n        {\n            return this.$store.state.viviendaseleccionada\n        }\n    },\n    mounted(){\n        this.cargafotos()\n        \n    },\n    methods:{\n           cargafotos()\n            {\n                //let fot = ''\n                let numfotos = this.$store.state.viviendaseleccionada.numfotos\n\n                for(let i = 1 ; i <= numfotos; i++)\n                {\n                  if(i <= 5)\n                  {\n                    let ob = {\n                        'id':i,\n                        'url':'https://fotos15.apinmo.com/8228/'+this.$store.state.viviendaseleccionada.cod_ofer+'/3-'+i+'.jpg'\n                    }\n                    this.fotosurl.push(ob)\n                  }\n    \n                }\n                //return fot\n                //console.log(this.fotosurl)\n            }\n    },\n\n    components: {\n      Formficha,\n      'money-format': MoneyFormat,\n       agile: VueAgile\n    },\n}\n</script>\n\n"],"sourceRoot":"src/components"}]}