v-if=\"viviendaseleccionada.precioinmo <= 0\">\n                            Precio a Consultar\n                        </span>\n                </p>\n              </div>\n            </div>\n\n            <div class=\"textos\">\n              <p class=\"descrip\">\n                <strong>Descripción</strong><br>\n                {{ viviendaseleccionada.nbconservacion }}\n              </p>\n            </div>\n\n            <div class=\"owl-carousel owl-theme\">\n              \n          <slider animation=\"fade\" height=\"420px\">\n            <slider-item\n              v-for=\"(foto, index) in fotosurl\"\n              :key=\"index\" \n              >\n              <img :src=\"foto.url\" />\n            </slider-item>\n          </slider>\n\n\n\n            </div>\n          </div>\n\n          <Formficha />\n\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\n\nimport Formficha from './Formficha.vue'\nimport MoneyFormat from 'vue-money-format'\nimport { Slider, SliderItem } from 'vue-easy-slider'\n\n\nexport default {\n    \n    name: 'Ficha',\n      data() {\n        return {\n            fotosurl: [],\n        };\n  },\n    computed: {\n        viviendaseleccionada() \n        {\n            return this.$store.state.viviendaseleccionada\n        },\n        \n\n    },\n\n    mounted(){\n        this.cargafotos()\n        \n    },\n    methods:{\n           cargafotos()\n            {\n                //let fot = ''\n                let numfotos = this.$store.state.viviendaseleccionada.numfotos\n\n                for(let i = 1 ; i <= numfotos; i++)\n                {\n               \n                    let ob = {\n                        'id':i,\n                        'url':'https://fotos15.apinmo.com/8228/'+this.$store.state.viviendaseleccionada.cod_ofer+'/3-'+i+'.jpg'\n                    }\n                    this.fotosurl.push(ob)\n                  \n    \n                }\n                //return fot\n                //console.log(this.fotosurl)\n            },\n            volver()\n            {\n                \n                this.$store.dispatch(\"cierraVivienda\");\n            }\n    },\n\n    components: {\n      Formficha,\n      'money-format': MoneyFormat,\n          Slider,\n    SliderItem,\n    },\n}\n</script>\n\n<style scoped>\n  a.volver{\n    color:#C10000;\n    font-size: 30px;\n   \n  }\n</style>"],"sourceRoot":"src/components"}]}