                <a href=\"\">\n                    <p class=\"venta\"> \n                         <money-format :value=\"vivienda.precioinmo\" locale='es' currency-code='EUR' /></p>\n                    <p class=\"poblacion\">\n                    <span>{{vivienda.ciudad}}</span><br>\n                    {{vivienda.nbtipo}}</p>\n                    <p class=\"datos\">\n                    <span>Habitaciones:</span> {{vivienda.habitaciones}}<br>\n                    <span>Superficie:</span> {{vivienda.m_cons}} m2<br>\n                    <span>Ba√±os:</span> {{vivienda.banyos}}<br>\n                    <span>Ref:</span> {{vivienda.cod_ofer}}\n                    </p>\n                    <p class=\"descrip\">{{vivienda.nbconservacion}}</p>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </section>\n</template>\n\n<script>\n\nimport MoneyFormat from 'vue-money-format'\n\nexport default {\n    \n  name: 'Resultado',\n  components:{\n    'money-format': MoneyFormat\n  },\n  computed: {\n    viviendas() \n    {\n        \n        let viviendas = [];\n        viviendas = this.$store.state.viviendas;\n        if(viviendas && viviendas.length > 0)\n        {\n            viviendas = viviendas.filter((item,index) => { return index > 0})\n        }\n        return viviendas\n    }\n  },\n  mounted() {\n    this.$store.dispatch(\"fetchDestacadas\",\"paginacion\");\n  },\n  methods:\n  {\n      seleccionaVivienda(item)\n      {\n          //console.log(item)\n          this.$store.dispatch(\"seleccionavivienda\",item);\n      }\n  }\n}\n</script>\n\n<style scoped>\n    .img{        \n        height: 320px;\n        background-color: #f3f3f3;\n    }\n</style>\n\n"]}]}