     }\n    },\n    mounted(){\n        this.cargafotos()\n        \n    },\n    methods:{\n           cargafotos()\n            {\n                //let fot = ''\n                let numfotos = this.$store.state.viviendaseleccionada.numfotos\n                console.log(this.$store.state.viviendaseleccionada)\n                for(let i = 1 ; i <= numfotos; i++)\n                {\n                    let ob = {\n                        'id':i,\n                        'url':'https://fotos15.apinmo.com/8228/'+this.$store.state.viviendaseleccionada.cod_ofer+'/3-'+i+'.jpg'\n                    }\n                    this.fotosurl.push(ob)\n                }\n                //return fot\n                console.log(this.fotosurl)\n            }\n    },\n\n    components: {\n    Formficha,\n    'money-format': MoneyFormat,\n        Slider,\n    SliderItem,\n    },\n}\n</script>\n\n"]}]}